<div class="project-list">

  {{#each this.model as |project|}}
    <button
      class="project-button"
      {{action "launchProject" project}}
    >
      {{project.title}}
    </button>
  {{/each}}

</div>

<div class="project-container">

  {{#if projectToDisplay}}
    {{!-- {{projectToDisplay.title}} --}}
      <ProjectCardNew @project={{projectToDisplay}} />

  {{else}}
    select project from list
  {{/if}}



  {{!-- <ProjectCardNew @project={{this.model}} /> --}}

</div>



